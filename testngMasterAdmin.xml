<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="SystemAdmin">
    <listeners>
        <listener class-name="listeners.AnnotationTransformer"/>
    </listeners>
    <test name="check systemAdmin">
        <parameter name="browserType" value="chrome"/>
        <classes>
           <class name="Admin.VerifyAddNewRoleInAdministration"/>
            <class name="Admin.VerifyRoleGetUploadedSuccessfully"/>
        </classes>
    </test>
    <test name="Designer-Create Framework/Library">
        <parameter name="browserType" value="chrome"/>
        <classes>
            <class name="Designer.VerifyNewFrameworkCreatedOnDesignerPage"></class>
        </classes>
    </test>
</suite>